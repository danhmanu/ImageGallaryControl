<UserControl x:Class="ImageControlCustom.SlideViewer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ImageControlCustom"
             mc:Ignorable="d" >
    <Grid Background="Black" ClipToBounds="True">
        <Image x:Name="slide1Control" Stretch="None" />
        <Image x:Name="slide2Control" Stretch="None" />
    </Grid>
</UserControl>
